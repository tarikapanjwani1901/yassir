/*******Our category box*****/
.category-row {
     text-align: center;
     background: #fff;
}
.commonttl-subttl {
     position: relative;
     text-align: center;
}
.commonttl-subttl .lightttl {
     letter-spacing: -1px;
     position: absolute;
     top: -19px;
     color: rgba(140, 23, 48, 0.1);
     text-transform: uppercase;
     font-size: 58px;
     width: 100%;
     font-weight: 700;
}
.commonttl-subttl h2 {
     font-size: 30px;
     text-align: center;
     font-weight: 700;
     color: rgba(0, 0, 0, 0.7);
     line-height: 38px;
     padding: 0 0 20px 0;
     margin: 0 0 20px 0;
     position: relative;
}
.commonttl-subttl h2:after {
     content: "";
     width: 60px;
     position: absolute;
     height: 4px;
     border-radius: 2px;
     background-color: #8c1730;
     margin: 0 auto;
     bottom: 0;
     left: 0;
     right: 0;
}
.commonttl-subttl span {
     font-size: 14px;
     line-height: 18px;
     color: rgba(0, 0, 0, 0.5);
     font-weight: 700;
     text-align: center;
     display: block;
     margin-bottom: 50px;
}
.category-row .category-box {
     width: 17%;
     min-height: 95px;
     border-radius: 5px;
     -webkit-border-radius: 5px;
     background-color: #8c1730;
     text-align: center;
     display: inline-block;
     margin: 15px;
     padding: 23px 10px;
}
.category-row .category-box:hover {
     background: #620a21;
}
.category-row .category-box span {
     color: #fff;
     display: block;
     font-size: 22px;
     line-height:22px;
     font-weight: 700;
     margin: 18px 0 0 0;
     text-transform: capitalize;
}
