body {
     padding: 0;
     margin: 0;
     background: #fff;
     font-family: 'Comfortaa', cursive;
     font-size: 14px;
     color: rgba(0, 0, 0, 1);
     font-weight:700;
}
a {
     transition: all 0.3s ease;
     -webkit-transition: all 0.3s ease;
}
a:hover,a:focus {
     text-decoration: none;
     outline: none;
}
button:hover,button:focus,input,input:hover,input:focus {
     outline: none;
     transition: all 0.3s ease;
     -webkit-transition: all 0.3s ease;
}
ul {
     padding: 0;
     margin: 0;
     list-style: none;
}
#main {margin-top: 100px;}
 .mg1{margin: 20px;}
}
