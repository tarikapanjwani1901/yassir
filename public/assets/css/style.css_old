/* image size */
.listing-img-pro {background-size: 100%;overflow: hidden;margin-top: 3px;height: 100%;position: relative;}
.featured-cvr{box-shadow: 2px 2px 2px #0404044d;position: absolute;
    width: 75%;
    background: #8c1730;
    transform: rotate(-49deg);
    transform-origin: 52% 238%;
    text-align: center;}
.featured-cvr h6{margin: 0;margin: 6px 0px 4px;color:#fff;}
  @-webkit-keyframes argh-my-eyes {
    0%   { background-color: #8c1730; }
    49% { background-color: #8c1730; }
    50% { background-color: #000; }
    99% { background-color: #000; }
    100% { background-color: #8c1730; }
  }
  @-moz-keyframes argh-my-eyes {
    0%   { background-color:#8c1730; }
    49% { background-color: #8c1730; }
    50% { background-color: #000; }
    99% { background-color: #000; }
    100% { background-color:#8c1730; }
  }
  @keyframes argh-my-eyes {
    0%   { background-color: #8c1730; }
    49% { background-color: #8c1730; }
    50% { background-color: #000; }
    99% { background-color: #000; }
    100% { background-color: #8c1730; }
  }
  .featured-cvr {
  -webkit-animation: argh-my-eyes 1s infinite;
  -moz-animation:    argh-my-eyes 1s infinite;
  animation:         argh-my-eyes 1s infinite;
}    

