/* width */
.services-content p::-webkit-scrollbar {
  width: 4px;

}

/* Track */
.services-content p::-webkit-scrollbar-track {
  background: #f1f1f1; 
}

/* Handle */
.services-content p::-webkit-scrollbar-thumb {
  background:#8c1730; 
  border-radius: 80px;
}

