nav,.loginbox {
     display: inline-block;
     text-align: right;
}
.menuicon{display: none;}
.navigation ul li {
     display: inline-block;
     position: relative;
     text-align: center;
         margin: 5px 7px;
            font-size: 14px;

}
.navigation ul li span {
     width: 0;
     height: 0;
     border-left: 4px solid transparent;
     border-right: 4px solid transparent;
     border-top: 6px solid #4a4a4a;
     position: relative;
     top: 0px;
     margin: 0 10px 0 5px;
     display:inline-block;
}
.navigation ul li:hover span {
     border-top: 6px solid #8c1730;
}
.navigation ul li ul {
     position: absolute;
     top: 100%;
     left: 0;
     display: none;
}
.navigation ul li:hover ul {
     display: block;
     padding: 0;
     margin: 0;
     background: #ba1f3c;
     min-width: 159px;
}

.navigation ul li a {
     color: #4a4a4a;
     font-family: 'Comfortaa', cursive;
     font-weight: 700;
     padding: 5px 2px;
     display: inline-block;
}
.navigation ul li a:hover {
     color: #8c1730;
     text-decoration: none;
}
.navigation ul li:hover ul li {
     padding: 0;
     display: block;
     text-align: left;
     border-bottom: 1px solid rgba(255, 255, 255, 0.4);
 margin:0;
     padding: 0;
     font-size:13px;
}
.navigation ul li:hover ul li:last-child {
     border-bottom: 0;
}
.navigation ul li:hover ul li a {
     padding: 9px 15px;
     color: #fff;
     display: block;
}
.navigation ul li:hover ul li a:hover {
     background: #620a21;
}
/*banner*/
.banner-row {
     position: relative;
}
.home .banner-row ,
.stickynav .inner-content{margin-top:100px;}
.banner-row .flexslider {
     position: relative;
     z-index: 0;
     border:0;
     margin:0;
}
.banner-row ul.slides li {
     position: relative;
     min-height: 400px;
     background-position: top center;
     background-repeat: no-repeat;
     background-size: cover;
}
.banner-row .bannertext,.inner-banner-row .bannertext {
     position: absolute;
     top: 50%;
     transform: translateY(-50%);
     -webkit-transform: translateY(-50%);
     max-width: 885px;
     left: 0;
     right: 0;
     margin: auto;
     text-align: center;
     display: block;
     z-index: 55;
     width: 95%;
}
.bannertext h1 {
     color: #fff;
     font-weight: 700;
     color: #fff;
     margin: 0 0 20px 0;
     padding: 0;
     line-height: 50px;
     font-size: 42px;
     z-index: 55;
}
.bannertext span {
     display: inline-block;
     color: #fff;
     font-size: 15px;
     font-weight: 400;
     line-height: 20px;
     max-width: 418px;
     margin: 0 auto;
}
.select2-selection__arrow{position:absolute;}
.bannertext .searchbox {
    display: block;
    max-width: 885px;
    width: 100%;
    border: 10px solid rgba(255, 255, 255, 0.4);
    border-radius: 35px;
    -webkit-border-radius: 35px;
    box-shadow: 0 0 39px rgba(0, 0, 0, 0.24);
    -webkit-box-shadow: 0 0 39px rgba(0, 0, 0, 0.24);
    margin: 20px auto 0 auto;
    background: #fff;
    -moz-background-clip: border;
    -webkit-background-clip: border;
    background-clip: border-box;
    -moz-background-clip: padding;
    -webkit-background-clip: padding;
    background-clip: padding-box;
    -moz-background-clip: content;
    -webkit-background-clip: content;
    background-clip: content-box;
    float: left;
    overflow: hidden;
}
.bannertext .searchbox input[type="text"] {
     padding: 10px 10px 10px 30px;
     background: #fff;
     display: inline-block;
     border: 0;
     border-radius: 35px 0 0 35px;
     float: left;
     margin: 5px 0;
     width: 30%;
}
select::-ms-expand {
     display: none;
}
.bannertext.sticky-tab{
position: fixed;
z-index:999999;
top:153px;
background: rgba(204, 204, 204,0.5);
max-width: 100%;
padding: 0;
width: 100%;
}
.bannertext.sticky-tab .searchbox{float: none; margin: 15px auto;}
.bannertext.sticky-tab h1{display: none;}
.bannertext.sticky-tab .stk{display:none;}
.bannertext .searchbox select {
     padding: 10px 30px 10px 10px;
     background: #fff;
     display: inline-block;
     -webkit-appearance: none;
     -moz-appearance: none;
     border: 0;
     position: relative;
     background: url(../images/Shape.png) 90% center no-repeat;
     border-left: 1px solid #ccc;
     width: 20%;
     float: left;
     margin: 5px 0;
     font-size:14px;
         font-family: 'Comfortaa', cursive;
         color:rgba(0, 0, 0, 0.70);
         font-weight:700;
}
.bannertext .searchbox button {
     width: 10%;
     float: left;
     border-radius: 0 35px 35px 0;
     background: #8c1730;
     border: 0;
     padding: 15px 10px;
     color: #fff;
}
.bannertext .searchbox button:hover {
     background: #620a21;
}
.flex-control-nav,.flex-direction-nav {
     display: none;
}
.inner-banner-row {
     position: relative;
     min-height: 300px;
     background-position: top center;
     background-repeat: no-repeat;
     background-size: cover;
}

